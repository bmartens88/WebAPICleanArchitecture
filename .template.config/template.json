{
  "$schema": "http://json.schemastore.org/template",
  "author": "Bas Martens",
  "classifications": ["Web", "WebAPI"],
  "name": "ASP.NET Core Web API Clean Architecture",
  "shortName": "webapicleanarch",
  "defaultName": "WebAPICleanArchitecture",
  "identity": "BasMartens.WebAPICleanArchitecture.CSharp",
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "sourceName": "BasMa.Api.Template",
  "preferNameDirectory": true,
  "primaryOutputs": [
    {
      "path": "BasMa.Api.Template.sln.sln"
    }
  ],
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/[Bb]in/**",
            "**/[Oo]bj/**",
            ".template.config/**/*",
            "**/*.filelist",
            "**/*.user",
            "**/*.lock.json",
            "**/.vs/**"
          ]
        }
      ]
    }
  ]
}